  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <mirrors>
    <mirror>
      <id>3ap-nexus</id>
      <mirrorOf>*</mirrorOf>
      <name>3AP Nexus release mirror</name>
      <url>https://jenkins-linode.3ap.ch/nexus/repository/maven-public</url>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <id>allow-snapshots</id>
      <activation>
        <activeByDefault>true</activeByDefault>
